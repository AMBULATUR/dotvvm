{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch SeleniumGenerator (OWIN)",
      "type": "clr",
      "request": "launch",
      "preLaunchTask": "build-command-line-net472",
      "program": "${workspaceFolder}/DotVVM.CommandLine/bin/Debug/net472/dotnet-dotvvm.exe",
      "cwd": "${workspaceFolder}/DotVVM.Samples.SeleniumGenerator.Owin",
      "args": [
        "gut"
      ],
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "Launch SeleniumGenerator (ANC)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-command-line-netcoreapp2.1",
      "program": "${workspaceFolder}/DotVVM.CommandLine/bin/Debug/netcoreapp2.1/dotnet-dotvvm.dll",
      "cwd": "${workspaceFolder}/DotVVM.Samples.SeleniumGenerator.ANC",
      "args": [
        "gut"
      ],
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "Launch Samples",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-samples",
      "program": "${workspaceFolder}/DotVVM.Samples.BasicSamples.AspNetCoreLatest/bin/Debug/netcoreapp3.0/DotVVM.Samples.BasicSamples.AspNetCoreLatest.dll",
      "args": [],
      "cwd": "${workspaceFolder}/DotVVM.Samples.BasicSamples.AspNetCoreLatest",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "^\\s*Now listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
