@viewModel DotVVM.Samples.BasicSamples.ViewModels.Sample48ViewModel, DotVVM.Samples.BasicSamples

<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />    
    <title>Hello from DotVVM!</title>
</head>
<body ClientIDMode="Static">

	<dot:Repeater DataSource="{value: Children}" WrapperTagName="ul" ID="first" ClientIDMode="Static">
		<ItemTemplate>
			<li>
				{{value: Name}}
			</li>
		</ItemTemplate>
	</dot:Repeater>
	
	<p ID="test2">{{value: ChangedValue}}</p>

	<dot:SpaContentPlaceHolder ID="MainContent" DefaultRouteName="Sample48" />

	<span ID="testResult">{{value: TestResult}}</span>

	<ul>
		<li><dot:RouteLink RouteName="Sample48" Text="Page A" /></li>
		<li><dot:RouteLink RouteName="Sample48_2" Text="Page B" /></li>
	</ul>

</body>
</html>